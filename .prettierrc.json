{
  "arrowParens": "always",
  "bracketSameLine": false,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": true,
  "printWidth": 80,
  "proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "semi": true,
  "singleAttributePerLine": false,
  "singleQuote": false,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useEditorConfig": false,
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "withNodeModules": false
}
